
package in.gov.lrit.asp.lritAPI;

import java.util.List;

public class LRITAPIDashBoard
{

    private List<ShipDetail> shipDetails = null;
    
    public LRITAPIDashBoard() {
    }

    /**
     * 
     * @param shipDetails
     */
    public LRITAPIDashBoard(List<ShipDetail> shipDetails) {
        super();
        this.shipDetails = shipDetails;
    }

    public List<ShipDetail> getShipDetails() {
        return shipDetails;
    }

    public void setShipDetails(List<ShipDetail> shipDetails) {
        this.shipDetails = shipDetails;
    }

    
    @Override
    public String toString() {
        return "ShipDetails [ShipDetails=" + shipDetails + "]";
    }
    
}
